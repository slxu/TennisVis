		pp_params.allowScriptAccess = "always";
		pp_params.wmode = "transparent";
		
		var pp_attributes = {};
		pp_attributes.id = "flashcontentb27b6e12a2bd4f30b3fe7d2246e636a1";
		swfobject.embedSWF("/media/flash/atpPlayerBio.swf", "flashcontentb27b6e12a2bd4f30b3fe7d2246e636a1", "311", "162", "9.0.124", "/media/flash/expressInstall.swf", pp_flashvars, pp_params, pp_attributes);
</script>


    </div>    

</div>




