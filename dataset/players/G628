		pp_params.bgcolor = "#ffffff";
		pp_params.quality = "best";
		pp_params.allowScriptAccess = "always";
		pp_params.wmode = "transparent";
		
		var pp_attributes = {};
		pp_attributes.id = "flashcontent45baba52e60a4398af7d0c8a13a7a729";
		swfobject.embedSWF("/media/flash/atpPlayerBio.swf", "flashcontent45baba52e60a4398af7d0c8a13a7a729", "311", "162", "9.0.124", "/media/flash/expressInstall.swf", pp_flashvars, pp_params, pp_attributes);
</script>


    </div>    

</div>




